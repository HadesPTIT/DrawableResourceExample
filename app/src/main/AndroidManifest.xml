<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.hades.drawableresourcedemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".BitmapActivity"/>
        <activity android:name=".ClipActivity"/>
        <activity android:name=".ShapeActivity"/>
        <activity android:name=".InsetActivity"/>
        <activity android:name=".LayerActivity"/>
        <activity android:name=".LevelActivity"/>
        <activity android:name=".NinePatchActivity"/>
        <activity android:name=".ScaleActivity"/>
        <activity android:name=".StateActivity"/>
        <activity android:name=".TransitionActivity"/>

    </application>



</manifest>